/*新增标题*/
.basetitle{
	height:70px !important;
	background:#E3F4FE !important;
	color:#209EE8 !important;
	display:flex;
	align-content: flex-start;
	align-items: center;
	-webkit-user-select: none; /* Chrome/Safari/Opera */
}
.basetitle .log{
	height:70px;
	overflow: hidden;
}
.appname{
	font-size:38px;
	font-family: "Microsoft YaHei";
    width: 65%;
    text-align: center;
    font-weight: 900;
    background-image:-webkit-linear-gradient(top,#6CC0F7,#3D9DDA); 
    -webkit-background-clip:text; 
    -webkit-text-fill-color:transparent; 
}
.basetitle .clock{
	width:6.5%;
	display:inline-block !important;
	padding-top:5px !important;
	line-height: 20px !important;
	font-size: 1rem !important;
	text-align:right;
}
.basetitle .clock >div{
	text-align:center;
}
.basetitle .clock span>div{
	text-align:center;
}
.basetitle .user{
	text-align:right;
	font-size:20px;
	width: 100px;
	font-weight: 600 !important;
}
.clocker{
	color:#DEF0FB;
	background:#209EE8;
	border-top-left-radius:2em;
	border-bottom-left-radius:2em;
	border-top-right-radius:2em;
	border-bottom-right-radius:2em;
	padding:0px 10px;
}
@keyframes show {
    0% {
        opacity: 0;
    }
    50% {
        opacity: 0.5;
    }
    100% {
        opacity: 1;
    }
}
@keyframes hide {
    0% {
        opacity: 1;
    }
    50% {
        opacity: 0.5;
    }
    100% {
        opacity: 0;
    }
}
.show-enter-active {
    animation: show .2s;
}
.show-leave-active {
    animation: hide .2s;
}
.show-enter, .show-leave-to {
    opacity: 0;
}
