/*业务字典页面弹出框的title和header样式开始*/
.nui-panel-header
{
    width:auto;
    height:35px;
    overflow:hidden;
    background:#f7f7f7;
    color:#201F35;
    border-bottom:solid 1px #bbb;
    cursor:default;
    position:relative;
    zoom:1;
}
.nui-panel-footer
{
    background:#f7f7f7;
}
.nui-panel-header-inner
{	
    padding:3px;
    padding-bottom:2px;
    padding-top:8px;
    padding-left:8px;
}
.nui-panel .nui-panel-title
{
    padding-top:0px\9;
    float:left;
    line-height:16px;
    line-height:18px\9;
    font-weight:700;
    font-size:16px    
}
.nui-window .nui-panel-header
{
    background:#f7f7f7;    
}
.nui-window .nui-panel-footer
{
    background:#f7f7f7;
}
.navbar-collapse{
  position:relative\9;
}

.navbar-collapse > .navbar-right{
  position:absolute\9;
  top:0\9;
  right:30px\9;
}

