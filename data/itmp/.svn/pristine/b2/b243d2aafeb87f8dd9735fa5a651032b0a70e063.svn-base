/*clip.js下面用于显示页面源代码的按钮*/
#source-button {
    position: absolute;
    top: 0;
    right: 0;
    z-index: 10000;
    font-weight: 700;
}
/*clip、btn-clip和copy-succ都是在clip.js中添加，但是已经被注释掉*/
.clip {
  position: relative;
  display: inline-block;
  margin-left:8px;
}
@media (min-width: 768px){
	.clip {
  	display: inline-block;
	}
}
.btn-clip{
z-index: 10;
display: block;
padding: 5px 8px;
font-size: 12px;
color: #767676;
cursor: pointer;
background-color: #fff;
border: 1px solid #e1e1e8;
border-radius: 0 4px 0 4px;
}
.btn-clip:hover{
	color:#fff;
	background:#000;
}
.copy-succ{
	z-index: 10;
	display: inline-block;
	opacity: 0;
	padding: 5px 8px;
	font-size: 12px;
	color: #fff;
	background-color:#000;
	cursor: pointer;
	border: 1px solid #e1e1e8;
	border-radius: 0 4px 0 4px;
	transition: all .5s;
	-webkit-transition: all .5s;
	-ms-transition: all .5s;
	-o-transition: all .5s;
	-moz-transition: all .5s;
}
/**
 * 实际项目应用场景  apply.css
 */
*{
	margin:0;
	padding:0;
	box-sizing: border-box;
}

/* **** */
body{
	font-family: "microsoft yahei",arial;
	background-color:transparent;
}
body .bs-example:after {
    content: "实例：" !important;
}
/*   apply/目录   */
/*bs-docs-section是指内容包含区域盒子模型，多用于搜索框box样式*/
.bs-docs-section{
	margin-bottom: 0;
    padding:15px;
	background-color:#f1f1f1;
}
.bs-docs-section  .navbar{
	border-bottom:1px solid #ddd;
}
.bs-docs-section .navbar .container-fluid .button-group {
	padding:8px 15px;
}
.bs-docs-section label{
	font-weight:400;
}
/*系统表头样式*/
/*patch为发起流程中盒子模型的样式*/
.navbar .navbar-brand,.patch  .navbar .navbar-brand,.patch.navbar .navbar-brand{
	font-size:14px;
	font-weight:bold;
	color:#000;
}
.navbar{
	margin-bottom:0;
	z-index: 1000;
	background-color:#fafafa;
}
.navbar:after{
	display: block;
    width: 4px;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-color: #004a86;
    z-index: 1000;
}
.navbar .navbar-collapse{
	overflow:hidden;
	border:0;
}
.navbar-collapse.collapse {
    display: block !important;
    height: auto !important;
    padding-bottom: 0;
    overflow: visible !important;
}
.navbar .nav-pills a{
	padding-top:15px;
	padding-bottom:15px;
}
.header.navbar,.header .navbar{
	padding:0 15px;
}
.header.navbar .navbar-brand,.header .navbar .navbar-brand{
	padding:0;
}
.header .navbar .navbar-brand img,.header.navbar .navbar-brand img{
	
	margin-top:5px;
}
/*隐藏发起流程中流程类别旁按钮的样式*/
.navbar-toggle{
	display:none;
}
/*右对齐浮动的样式 表头、向下显示内容图标的样式*/
/*cuc.css中有media修饰该属性*/
.navbar-nav > li {
	float:left;
}
.navbar-right{
	float:right;
}
.navbar-header {
    float: left;
}
/*表头搜索框的样式*/
.navbar-form{
	margin:0;
	margin-top:5px;
	padding:3px 15px;
	border:0;
}
.navbar-form .form-group {
    display: inline-block;
    margin-bottom: 0;
    vertical-align: middle;
}
/*表头上面移动鼠标到小图标上显示内容的样式*/
.navbar-nav .open .dropdown-menu{
    left: auto;
    right: 0;
    position:absolute;
    margin-top:2px;
    background-color:#fff;
}
/*系统左侧列表样式*/
.mainstay{
	position:relative;
}
.mainstay:before,.mainstay:after{
	content:"";
	display:table;
}
.side-left{
	position:fixed;
	top:0;
	left:0;
	bottom:0;
	z-index: 800;
}
.side-bar{
	position:relative;
	padding-top:50px;
	width:190px;
	height:100%;
	z-index: 1000;
	background:#004F83;
	transition: all .8s; 
	-webkit-transition: all .8s; 
	-ms-transition: all .8s; 
	-moz-transition: all .8s; 
}
/*点击收起左侧列表图标的图标样式*/
.side-toggle{
	display:block;
	width:100%;
	position: relative;
	color:#fff;
    line-height:30px;
    padding:0;
    background-color: #1a6190;
    background-image: none;
    border: 0;
    border-radius: 3px;
    outline: none;
}
.side-toggle span{
	color:#aeb9c2;
}
/*bar-flex side-icon暂无涉及*/
.side-left.bar-flex{
	opacity:1;
}
.bar-flex .side-toggle{
	text-align:right;
	padding-right:20px;
}
/*.bar-flex .side-text{
	display:none;
}*/
.bar-flex .side-pills > li > a{
	/*text-align:right;*/
}
/*.side-toggle:hover{
	background:#fff;
}*/
/*.side-toggle:focus,.side-toggle:active{
	background-color:#ddd;
	border:0;
}
.side-toggle:focus .side-icon,.side-toggle:active .side-icon{
	background:#888;
}*/
.side-toggle .side-icon{
	display:inline-block;
	width:2px;
	height:15px;
	background:#ddd;
}
/*左侧列表内容区样式*/
.side-pills,.side-menu{
	width:100%;
	list-style: none;
	padding:0;
	height:auto;
	position:relative;
	z-index: 1000;
    overflow-x:hidden;
}
.side-menu{
    overflow-y:scroll;
}
.side-menu .badge{
    font-size:12px;
    font-weight:400;
    color:#fff;
    position:absolute;
    right:32px;
    top:11px;
}
.side-menu > li{
	width:207px;
}
.side-pills > li > a,.side-menu > li > a,.fully  li  a{
	display:inline-block;
	width:100%;
	color:#fff;
	line-height:40px;
	text-decoration:none;
	white-space: nowrap;
	/*overflow:hidden;*/
}
.side-pills > li > a  .side-fa,.side-menu > li > a .side-fa{
	margin-top:2px;
	transition: all .8s; 
	-webkit-transition: all .8s; 
	-ms-transition: all .8s; 
	-moz-transition: all .8s; 
}
.side-pills > li > a:hover,.side-pills > li > a:active,.side-pills > .active > a,.side-pills > li > a:focus{
	background-color:#00375E;
}
.side-menu > li > a{
    position: relative;
	padding-left:15px;
	padding-right:17px;
	background-color:#002C4B;
}
.side-menu > li > a:hover,.side-menu > .active > a,.side-menu > li > a:active,.side-menu > li > a:focus{
	background-color:#0099E3;
}
/*一级标题左侧指向箭头图标样式*/
.nav-icon{
	display:inline-block;
	width:50px;
	text-align:center;
	position:relative;
}
.nav-icon .fa{
	transition: all .8s; 
	-webkit-transition: all .8s; 
	-ms-transition: all .8s; 
	-moz-transition: all .8s; 
}
.side-rotate{
	transform: rotate(90deg);
	-webkit-transform: rotate(90deg);
	-ms-transform: rotate(90deg);
	-moz-transform: rotate(90deg);
}
/*标题文字样式*/
.side-text{
	display:inline-block;
	font-size: 16px;
	/*float:left;*/
}
.side-text-second{
	display:inline-block;
	font-size: 14px;
	/*float:left;*/
}
/*系统右侧列表隐藏样式*/
.fully{
	list-style: none;
	padding-top:50px;
	position:absolute;
	right:100%;
	min-width:150px;
	left:190px;
	top:0;
	bottom:0;
	margin:0;
	background: #dcdfe3;
	z-index: 100;
}
.scroll-none{
	width:100%;
	overflow:hidden;
}
.fully ul li:first-child{
	height:30px;
}
.fully  li  a{
	color:#000;
	padding-left:30px;
}
.fully .fully-title{
	margin:0;
	font-size:16px;
	line-height:50px;
	padding-left:15px;
	font-weight: 600;
}
.fully  li  a:hover,.fully li  a:focus,.fully li  a:active{
	background-color:#e6e6e6;
}
.fully  .active  a{
	background-color:#f1f1f1;
}
.fully .btn-flex{
	position:absolute;
	width:25px;
	line-height:50px;
	right:0px;
	top:50%;
	margin-top:-25px;
	text-align:center;
	background:#F7F7F7;
	border-radius: 3px 0 0 3px;
	cursor: pointer;
}
.fully .btn-flex.on{
	background-color:#fff;
	border-radius: 0 3px 3px 0;
	right:-25px;
}
/*系统内容区样式*/
.box{
    position:fixed;
    top:50px;
    bottom:0;
    left:0;
    right:0;
	width:100%;
	height:auto;
	background-color:#f1f1f1;
}
.box:after,.box:before{
	content:"";
	display:table;
}
.box #apply{
    position:absolute;
    top:40px;
    left:0;
    right:0;
    bottom:0;
}
/*首页导航信息样式*/
.bread{
    background-color:#E5E9EC;
    padding-left:15px;
    overflow: hidden; 
}
.bread > a,.bread > span{
    display:inline-block;
    float:left;
    padding:10px 0;
    color:#686868;
    font-size:14px;
    text-decoration: none;
}
.bread > .active{
    color:#004a86;
}
/*内容容器的样式*/
.box-container{
	position:relative;
	height:100%;
	background-color:#f1f1f1;
}
.area{
	position:relative;
	background-color:#f1f1f1;
	width:100%;
	height:100%;
}
/**/
.area .navbar .button-group{
	padding-top:8px;
	padding-bottom:8px;
}
.content{
	border-bottom:1px solid #ddd;
	padding:15px;
}
/*搜索框样式*/
.form-control:focus{
    color:#000;
}
/**/
.sj-menu{
    color:#fff;
    width: 100%;
    padding: 0;
    height: auto;
    position: relative;
    z-index: 1000;
    background-color:#002c48;
    overflow-x: hidden;
}
.sj-menu > li > a {
    width:100%;
    padding-left:68px;
    position: relative;
    display: inline-block;
    color: #fff;
    line-height: 40px;
    text-decoration: none;
    white-space: nowrap;
    background-color:#002c48;
}
.sj-menu > li > a:hover {
    background-color:#0099E3;
    color:#fff;
}
.sj-menu .middot{
    display:inline-block;
    font-size:30px;
    height:40px;
    line-height:40px;
    position:absolute;
    top:0;
    left:52px;
}
