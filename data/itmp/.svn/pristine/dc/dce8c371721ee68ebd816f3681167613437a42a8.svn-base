package com.cgn.itmp.dailyandoverhaul.authorization;

public class Email {
	
	// 开发和测试环境默认发送给此用户
	private String alowUser;
	
	// 邮件内容
	private String body;
	
	// 抄送人
	private String cc;
	
	// 邮件标题
	private String subject;
	
	// 主送人
	private String to;

	public String getAlowUser() {
		return alowUser;
	}

	public void setAlowUser(String alowUser) {
		this.alowUser = alowUser;
	}

	public String getBody() {
		return body;
	}

	public void setBody(String body) {
		this.body = body;
	}

	public String getCc() {
		return cc;
	}

	public void setCc(String cc) {
		this.cc = cc;
	}

	public String getSubject() {
		return subject;
	}

	public void setSubject(String subject) {
		this.subject = subject;
	}

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}
	
	

}
