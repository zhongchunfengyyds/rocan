<?xml version="1.0" encoding="UTF-8"?>
<!-- author:Administrator -->
<sqlMap>
    <parameterMap class="commonj.sdo.DataObject" id="parameterMap">
        <parameter javaType="date" jdbcType="DATE" property="dateType"/>
    </parameterMap>
    <resultMap class="commonj.sdo.DataObject" id="resultMap">
        <result column="TYPEID" javaType="string" property="typeId"/>
    </resultMap>
    <select id="select_id" parameterMap="parameterMap" resultMap="resultMap"><![CDATA[sql statement]]></select>
	    
	  <select id = "selectJd" parameterMap="parameterMap" resultMap="resultMap" >
	        select 
				TD_DXGL_JDDICT.ID, TD_DXGL_JDDICT.NAME 
				from TD_DXGL_JDDICT;
	    </select>   
	    <select id = "selectJz" parameterMap="parameterMap" resultMap="resultMap">
	       select 
			TD_DXGL_JIZUDICT.ID, TD_DXGL_JIZUDICT.NAME,TD_DXGL_JIZUDICT.TYPE 
			from TD_DXGL_JIZUDICT 
			WHERE 
			TD_DXGL_JIZUDICT.JDID = #jdid#		
	    </select>
      <select id = "getSelectRysxys"   resultClass="commonj.sdo.DataObject">
	    		select DICTID id,DICTNAME name from EOS_DICT_ENTRY where DICTTYPEID='REASON1'
	   </select>
	   <select id = "getSelectRysxysX" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
	    		 select DICTID id,DICTNAME name from EOS_DICT_ENTRY where PARENTID=#parentid#
	   </select>
    <select id = "getSelectDxrl" parameterClass="java.util.List" resultClass="commonj.sdo.DataObject">
	    select  datediff(day,t1.startTime,t1.endTime) dxgq, t1.uuid,t1.jdid,t1.dxlc,t1.startTime,t1.endTime,t1.scr,t1.scsj,t1.isdelete,t1.xh,t1.jzid,t1.addTime,
		j.name from T_DXGL_DXRL t1 
		LEFT JOIN TD_DXGL_JDDICT j ON t1.jdid=j.ID
	    where t1.isdelete='0'
	    and t1.jdid 
    	in
      <iterate open="(" close=")" conjunction=",">
			#paramList[]#
	</iterate>
	
	   </select>
      <select id = "getJdInfoByName" parameterClass="java.util.List" resultClass="commonj.sdo.DataObject">
	   		  select ID
       from 
       TD_DXGL_JDDICT  where isdelete = '0'
	    and name
    	in
      <iterate open="(" close=")" conjunction=",">
			#paramList[]#
	 </iterate>
	
	   </select>
	   
	   
	   <select id = "getalltime" parameterClass="java.lang.String" resultClass="commonj.sdo.DataObject">
	   		SELECT startTime,endTime FROM T_DXGL_DXRL where xh=#xh#
	   </select>
	   
	   <select id = "edititemmanage" parameterClass="java.lang.String" resultClass="commonj.sdo.DataObject">
	   		SELECT t1.*,t2.FILE_PATH
			as filepath,t2.FILE_REAL_NAME as fileRealName FROM TD_DXGL_XMGLTREE t1
			left join T_COMMON_FILEUPLOAD t2 on t1.fileid = t2.file_id
			where relationid=#id#
			order by number
	   </select>
	   <select id ="itmpcount" parameterClass="java.lang.String" resultClass="commonj.sdo.DataObject">
	   		SELECT max(orderindex) as orderindex from TD_DXGL_XMGL WHERE ISDELETE='0' AND jd=#jd#
	   </select>
	    <select id ="count" parameterClass="java.lang.String" resultClass="commonj.sdo.DataObject">
	   		SELECT count(1) as count from TD_DXGL_XMGL WHERE ISDELETE='0' AND jd=#jd#
	   </select>
    	<delete id="deleterelation" parameterClass="java.util.Map" >
	    	delete TD_DXGL_XMGLTREE where relationid = 
	    	<isNotNull  property="relationid">
	           #relationid#
           </isNotNull>
 	</delete>
	   
    
</sqlMap>