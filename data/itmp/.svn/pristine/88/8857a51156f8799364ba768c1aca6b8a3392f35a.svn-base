* {
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

    html, body{
        margin:0;padding:0;border:0;width:100%;height:100%;overflow:hidden;
    }

.mini-grid-cell{border-right:none!important;}
.mini-tree-treeLine .mini-tree-indent{display:none;} 
.mini-tree-treecell .mini-grid-cell-inner{border-bottom: 1px solid #e7e7e7;padding:8px 0 8px 0px;text-indent:12px;}
.mini-tree-treeLine .mini-tree-expand .mini-tree-node-ecicon{position: absolute;left:5px;}
.mini-tree-treeLine .mini-tree-collapse .mini-tree-node-ecicon{position: absolute;left:5px;}

.mini-tree-treeLine .mini-tree-node-ecicon{background: transparent;}
.mini-tree-treeLine .mini-tree-expand .mini-tree-node-ecicon{background:none !important;}
.mini-tree-treeLine .mini-tree-collapse .mini-tree-node-ecicon-last{background:none !important;}
.mini-tree-treeLine .mini-tree-collapse .mini-tree-node-ecicon{background:none !important;}
 .mini-tree-selectedNode .mini-tree-nodeshow{border: none!important;background:transparent!important;color:#fff;}
