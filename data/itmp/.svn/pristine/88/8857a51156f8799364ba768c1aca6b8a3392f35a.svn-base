.mini-buttonedit .mini-buttonedit-border .mini-buttonedit-input{padding-left:6px!important;}
.mini-buttonedit-border{padding-right:0px!important;}

.mini-tree-nodetext{padding-right:0px;}

.navbar-inverse .navbar-nav>li>a {
	padding-top: 12px;
	padding-bottom: 15px;
}

.navbar {
	height: 50px;
}

.navbar-brand {
	padding: 0;
	padding-top: 5px;
}

.navbar-inverse .navbar-nav>li>a {
	font-size: 14px;
}

.navbar-right {
	margin-top: 0px\9;
}

.modal .modal-dialog.modal-lg {
	margin: 30px auto\9;
}

.clip,.copy-succ {
	display: none\9;
}

.navbar-default:after {
	background-color: #fff;
}

.navbar-blue:after {
	background-color: #3c8dbc;
}

.navbar.navbar-blue {
	background-color: #3c8dbc;
}

.navbar.navbar-blue .fa {
	color: #fff;
}

.nav>li>a:hover,.nav>li>a:focus {
	background-color: #002E53;
}

.second-navbar .navbar-nav>.open>a,.second-navbar .navbar-nav>.open>a:hover,.second-navbar .navbar-nav>.open>a:focus
	{
	background-color: #002E53;
}

.navbar-default .navbar-nav>.logo-text>a,.navbar-default .navbar-nav>.logo-text>a:hover
	{
	color: #fff;
	font-size: 14px;
}

.navbar-default .navbar-nav>li>a.navbar-circle {
	border-radius: 50%;
	-webkit-border-radius: 50%;
	width: 30px;
	height: 30px;
	padding: 0;
	line-height: 25px;
	margin-top: 12px;
	color: #fff;
	border: 2px solid #fff;
	text-align: center;
	font-size: 10px;
}


.navbar-default .navbar-nav>li>a.navbar-circle .fa {
	font-size: 12px;
}

.navbar-default .navbar-nav>li>a.navbar-circle:hover,.navbar-default .navbar-nav>li>a.navbar-circle:hover .fa
	{
	background: #f00;
	color: #004A86;
}

.side-menu>li>a {
	padding-left: 0;
}

.wresore>.dropdown-menu>li>a {
	font-size: 12px;
}
/*业务字典页面弹出框的title和header样式开始*/
.nui-panel-header
{
    width:auto;
    height:35px;
    overflow:hidden;
    background:#f7f7f7;
    color:#201F35;
    border-bottom:solid 1px #bbb;
    cursor:default;
    position:relative;
    zoom:1;
}
.nui-panel-footer
{
    background:#f7f7f7;
}
.nui-panel-header-inner
{	
    padding:3px;
    padding-bottom:2px;
    padding-top:8px;
    padding-left:8px;
}
.nui-panel .nui-panel-title
{
    padding-top:0px\9;
    float:left;
    line-height:16px;
    line-height:18px\9;
    font-weight:700;
    font-size:16px    
}
.nui-window .nui-panel-header
{
    background:#f7f7f7;    
}
.nui-window .nui-panel-footer
{
    background:#f7f7f7;
}
.navbar-collapse{
  position:relative\9;
}

.navbar-collapse > .navbar-right{
  position:absolute\9;
  top:0\9;
  right:30px\9;
}

/*业务字典页面弹出框的title和header样式结束*/
	/*选人选部门弹出框样式*/
	.nui-window .nui-panel-header
	{
	    background:#fafafa;    
	}
	.nui-panel .nui-panel-title
	{
	    padding-top:0px;
	    float:left;
	    line-height:16px;
	    line-height:18px;
	    font-weight:600;
	    font-size:14px    
	}
	.nui-window-drag {
	    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
	}
	.topNav
    {
        position:absolute;right:8px;top:12px;        
        font-size:12px;
        line-height:25px;
    }
    .topNav a
    {
        text-decoration:none;        
        font-weight:normal;
        font-size:12px;
        line-height:25px;
        margin-left:3px;
        margin-right:3px;
        color:#333;        
    }
    .topNav a:hover
    {
        text-decoration:underline;        
    }  
    
