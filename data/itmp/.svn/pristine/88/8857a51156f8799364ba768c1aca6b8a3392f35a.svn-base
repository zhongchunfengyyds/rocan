 .mini-tree-node-hover .mini-tree-nodeshow{border: none!important;background:transparent!important;/* color:#fff; */}

.mini-tree-selectedNode{background: #3c8dbc!important;color:#fff!important;}
.mini-tree-expand .mini-tree-folder{background-image:url(../img/wd.png)!important;}
.mini-tree-folder{background-image:url(../img/zz.png)!important;}
.mini-tree-leaf{width:20px;height:20px;background-image: url(../img/leaf.png)!important;background-size:contain;}
