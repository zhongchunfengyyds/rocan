<sqlMap>
    <parameterMap class="commonj.sdo.DataObject" id="parameterMap">
        <parameter javaType="date" jdbcType="DATE" property="dateType"/>
    </parameterMap>
    <resultMap class="commonj.sdo.DataObject" id="resultMap">
        <result column="TYPEID" javaType="string" property="typeId"/>
    </resultMap>
    <select id="select_id" parameterMap="parameterMap" resultMap="resultMap"><![CDATA[sql statement]]></select>
    
    <select id="updateProcessId" parameterClass="java.util.Map">
    	update T_DGGL_NDJHHD_DX set processInstId = #processinstid# where id = #uuid#
    </select>
    
    <select id="queryIdByProcessInstId" parameterClass="java.lang.String" resultClass="commonj.sdo.DataObject">
    	select id from T_DGGL_NDJHHD_DX  where processInstId = #processinstid#
    </select>
    
    <select id="queryLegendByName" parameterClass="java.util.Map" resultClass="commonj.sdo.DataObject">
    select CKTLBH,CKTLMC,fildid from T_CGN_DGGL_CKTLGL 
		where ISDELETE = 0 
		and jd = #jdid# 
		and jzid = #jzid# 
       <isNotNull prepend=" AND " property="legendNames">
     	 	 CKTLBH in  
   		   <iterate  open="("  close=")"  conjunction="," property="legendNames">
           		#legendNames[]#
      		</iterate>   
    	</isNotNull>
    </select>
</sqlMap>